<h1>All Kittens</h1>

<% @kittens.each do |kitten| %>
  <h3>Name: <%= link_to kitten.name, kitten_path(kitten) %></h3>
  <p>Age: <%= kitten.age %></p>
  <p>Softness: <%= kitten.softness %></p>
  <p>Cuteness: <%= kitten.cuteness %></p>
  <%= link_to "Delete kitten", kitten_path(kitten), method: :delete %><br><br>
<% end %>
